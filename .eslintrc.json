{
    "settings": {
      "react": {
        "version": "16.13.1" // здесь нужно указать версию React
    }
    },
    "extends": [
      "eslint:recommended",
      "plugin:react/recommended",
      "plugin:react/recommended",
      "plugin:@typescript-eslint/recommended",
      "prettier",
      "plugin:react-hooks/recommended",
      "plugin:prettier/recommended"
    ],
    "plugins": [
      "react"
    ],
    
    "parserOptions": {
      "ecmaVersion": 2021,
      "sourceType": "module",
      "ecmaFeatures": {
        "jsx": true
      }
    },
    "env": {
      "es6": true,
      "browser": true,
      "node": true
    },
    "rules": {
        "prettier/prettier": ["error", { "endOfLine": "auto" }]
    }
    
  }
  